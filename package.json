{
  "name": "@xiodone/eslint-config",
  "type": "module",
  "version": "0.0.1-beta.1",
  "packageManager": "bun@1.1.18",
  "description": "XioDone's ESLint config.",
  "author": "XioDone",
  "homepage": "https://github.com/XioDone/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XioDone/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/XioDone/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "bun run build",
    "tsup:build": "tsup-node",
    "build": "bun --bun unbuild",
    "release": "bumpp && npm publish",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "c": "rm -rf node_modules pnpm-lock.yaml package-lock.json bun.lockb dist"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.0",
    "@types/node": "^20.14.10",
    "@unocss/eslint-plugin": "^0.61.3",
    "@vitejs/plugin-vue": "^5.0.5",
    "bumpp": "^9.4.1",
    "eslint": "npm:eslint-ts-patch@9.5.0-0",
    "eslint-ts-patch": "^9.5.0-0",
    "lint-staged": "^15.2.7",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.61.3",
    "unoist": "^0.0.3",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.2",
    "vite": "^5.3.3",
    "vite-plugin-url-copy": "^1.1.4",
    "vue": "^3.4.31"
  },
  "simple-git-hooks": {
    "pre-commit": "bun lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
